<?xml version="1.0" encoding="UTF-8"?>
<!-- 主要用来适配，有的机型AEC，AGCNS  -->
<devices>
	<device>
        <manufacturer>samsung</manufacturer><!-- 设备制造厂商  全部小写 -->
		<model>gt-i9300</model><!-- 设备型号 全部小写 -->
		<sdk_leve>16</sdk_leve>	<!-- 手机对应SDK的版本号 -->
		<agc>true</agc><!-- 默认是true --> 
		<aec>false</aec><!-- 默认是true --> 
		<ns>false</ns><!--  默认是true -->
		<force_java_audio>true</force_java_audio><!-- 是否强制使用java采集 默认是false --> 
	</device>
	
	<device>
        <manufacturer>nokia</manufacturer><!-- 设备制造厂商  全部小写 -->
		<model>nokia_x</model><!-- 设备型号 全部小写 -->
		<sdk_leve>16</sdk_leve>	<!-- 手机对应SDK的版本号 -->
		<agc>true</agc><!-- 默认是true --> 
		<aec>true</aec><!-- 默认是true --> 
		<ns>true</ns><!--  默认是true -->
		<force_java_audio>true</force_java_audio><!-- 是否强制使用java采集 默认是false --> 
	</device>
</devices>